<launch>
    <include file="$(find franka_gazebo)/launch/panda.launch">
        <arg name="x" value="-0.5" />
        <arg name="world" value="$(find franka_gazebo)/world/stone.sdf" />
        <arg name="controller" value="cartesian_impedance_example_controller" />
        <arg name="rviz" value="false" />
    </include>

    <node pkg="diff_planner" type="diff_planner" name="diff_planner" respawn="false">
        <param name="mock" value="true" />
        <remap from="/setpoint" to="cartesian_impedance_example_controller/equilibrium_pose" />
    </node>
</launch>